<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:drawable="@color/list_normal" android:state_enabled="false" android:state_focused="true" />

    <!--Al presionar -->
    <item android:drawable="@color/list_pressed" android:state_pressed="true" />

    <!--Al presionar long -->
    <item android:drawable="@color/list_pressed_long" android:state_focused="true" android:state_pressed="true" />
    <item android:drawable="@color/list_pressed_long" android:state_focused="false" android:state_pressed="true" />
    <!--Al ser seleccionada -->
    <item android:drawable="@color/list_active" android:state_focused="true" />

